<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.shlanbao.tzsc.pms.cos.spare.beans.CosSparePartsBean" table="COS_SPARE_PARTS"   >
        <id name="id" type="java.lang.String">
            <column name="id" length="50" />
            <generator class="uuid.hex" />
        </id>
        <property name="eqpTypeId" type="java.lang.String" >
            <column name="eqp_type" length="50" />
       </property> 
        <property name="unitId" type="java.lang.String">
            <column name="unit"  length="50" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="NAME" length="50" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="REMARK" length="250" />
        </property>
        <property name="attr1" type="java.lang.String">
            <column name="ATTR1" length="50" />
        </property>
        <property name="attr2" type="java.lang.String">
            <column name="ATTR2" length="50" />
        </property>
        <property name="price" type="java.lang.Double">
            <column name="PRICE" scale="4"/>
        </property>
        <property name="type" type="java.lang.String">
            <column name="TYPE" length="50" />
        </property>
    </class>
</hibernate-mapping>