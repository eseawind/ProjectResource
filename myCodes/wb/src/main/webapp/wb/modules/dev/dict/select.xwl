{"title":"查询","inframe":false,"hidden":false,"roles":{"demo":1},"pageLink":"","children":[{"configs":{"serverScript":"var tag, rs;\ntry {\n  rs = app.run('select * from ' + app.get('schemTableName') + ' where 1=0', {\n    jndi: app.get('jndi')\n  });\n  tag = DbUtil.getFields(rs.getMetaData(), null, null, null).toString();\n} catch (e) {\n  //如果表已经被删除则FIELD_NAME下拉项返回空\n  tag = '[]';\n}\n\napp.output('select * from WB_DICT where TABLE_NAME={?TABLE_NAME?}', {\n  dictTableNames: 'WB_DICT',\n  tag: 'tableFields:' + tag //用于对话框FIELD_NAME项下拉提取字段\n});","itemId":"module"},"children":[],"type":"module","expanded":true}],"iconCls":""}