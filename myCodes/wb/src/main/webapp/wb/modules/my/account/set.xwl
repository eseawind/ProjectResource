{"title":"设置帐户","inframe":false,"hidden":false,"roles":{"basic":1},"pageLink":"","children":[{"configs":{"serverScript":"var session, password = app.get('PASSWORD');\napp.set({\n  PASSWORD: Encrypter.getMD5(password)\n});\napp.run('update WB_USER set DISPLAY_NAME={?nvarchar.DISPLAY_NAME?}' +\n  (password.equals('******') ? '' : ',PASSWORD={?varchar.PASSWORD?}') +\n  ',EMAIL={?varchar.EMAIL?},USE_LANG={?nvarchar.USE_LANG?} where USER_ID={?sys.user?}'\n);\nsession = request.getSession(true);\nsession.setAttribute('sys.lang', Wb.opt(app.get('USE_LANG'), 'auto'));\nsession.setAttribute('sys.dispname', app.get('DISPLAY_NAME'));","itemId":"module"},"children":[],"type":"module","expanded":false}],"iconCls":""}