{"title":"生成Excel文件","inframe":false,"hidden":false,"roles":{"demo":1,"default":1,"basic":1},"pageLink":"","children":[{"configs":{"serverScript":"var params = app.get(),\n  ext = com.wb.tool.DataOutput.getExtName();\nif (Wb.isEmpty(params.filename))\n  params.filename = 'data' + ext;\nelse\n  params.filename = params.filename + ext;\nresponse.setHeader(\"content-type\", \"application/force-download\");\nresponse.setHeader(\"content-disposition\", \"attachment;\" + WebUtil.encodeFilename(request, params.filename));\ncom.wb.tool.DataOutput.outputExcel(request, response.getOutputStream(),\n  new org.json.JSONArray(params.headers), new org.json.JSONArray(params.rows),\n  Wb.opt(params.title, null), Wb.opt(params.dateFormat, 'Y-m-d'), Wb.opt(params.timeFormat, 'H:i:s'), false);\nresponse.flushBuffer();","itemId":"module"},"children":[],"type":"module","expanded":false}],"iconCls":""}