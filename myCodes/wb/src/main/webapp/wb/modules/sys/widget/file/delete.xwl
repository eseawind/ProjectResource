{"title":"删除文件","inframe":false,"hidden":false,"roles":{},"pageLink":"","children":[{"configs":{"serverScript":"var rows = Wb.decode(app.get('fileGrid')),\n  path = new File(app.get('path')),\n  FileUtils = org.apache.commons.io.FileUtils;\nWb.each(rows, function(row) {\n  if (!FileUtils.deleteQuietly(new File(path, row.text)))\n    Wb.error('删除 \u201c' + row.text + '\u201d 失败。');\n});","itemId":"module"},"children":[],"type":"module","expanded":false}],"iconCls":""}