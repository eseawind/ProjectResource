{"title":"查询人员","inframe":false,"hidden":false,"roles":{"demo":1},"pageLink":"","children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"select\na.REAL_NAME,a.BIRTH_DATE,a.SEX,a.VACATION_HOURS,b.DEPT_NAME\nfrom WB_EMPLOYEE a, WB_EMPLOYEE_DEPT b\nwhere a.DEPT_ID=b.DEPT_ID order by a.REAL_NAME","fieldsTag":"[{\n  name: 'VACATION_DAYS',\n  type: 'float',\n  convert: function(value, record) {\n    /* 如果从数据库中提取数据，建议在SQL表达式中直接生成计算列，如select a, b, a + b as \"c\" from table */\n    if (value)\n      return value;\n    else\n      return record.get('VACATION_HOURS') / 8;\n  }\n}]","itemId":"dataprovider1"},"children":[],"type":"dataprovider","expanded":false}],"type":"module","expanded":true}],"iconCls":""}