{"title":"处理上传","inframe":false,"hidden":false,"roles":{},"pageLink":"","children":[{"configs":{"serverScript":"var os, files = app.getFiles('file1').concat(app.getFiles('file2')).concat(app.getFiles('file3'));\nvar IOUtils = org.apache.commons.io.IOUtils;\nWb.each(files, function(file) {\n  if (!file) return;\n  var os = new java.io.FileOutputStream(new File(Base.path, file.name)); //在应用根目录创建文件输出流\n  try {\n    //file.size为上传的文件大小\n    IOUtils.copy(file.stream, os);\n  } finally {\n    //file.stream可不用关闭，系统会自动关闭\n    os.close();\n  }\n});","itemId":"module"},"children":[],"type":"module","expanded":true}],"iconCls":""}